{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": ["error", 2],
    "max-len": "off",
    "no-invalid-this": "off",
    "require-jsdoc": "off"
  }
}
